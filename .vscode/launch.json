{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "inspect-brk",
      "stopOnEntry": false,
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["inspect-brk:9229"],
      "cwd": "${workspaceFolder}",
      "outputCapture": "std",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
